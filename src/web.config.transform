        <section name="Geta.EPi.Imageshop" type="Geta.EPi.Imageshop.Configuration.ImageshopConfigurationSection, Geta.EPi.Imageshop" />
    </configSections>
    <episerver.shell>
        <protectedModules>
            <add name="geta-epi-imageshop" />
        </protectedModules>
    </episerver.shell>
    <Geta.EPi.Imageshop>
        <settings
            token="AddYourTokenHere"
