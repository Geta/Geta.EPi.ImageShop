<?xml version="1.0"?>
<package >
    <metadata>
        <id>Geta.EPi.Imageshop</id>
        <version>$version$</version>
        <title>Geta.EPi.Imageshop</title>
        <authors>Geta</authors>
        <owners>Geta</owners>
        <projectUrl>https://github.com/Geta/Geta.EPi.Imageshop</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Imageshop Episerver integration. Contains property editor for single image and video and multiple images and videos as well as a TinyMCE plugin. See https://github.com/Geta/Geta.EPi.Imageshop for more details.</description>
        <releaseNotes></releaseNotes>
        <copyright></copyright>
        <tags>Episerver, Imageshop</tags>
        <dependencies>
          <dependency id="EPiServer.Framework" version="11.3.3" />
          <dependency id="EPiServer.CMS.Core" version="11.3.3" />
          <dependency id="EPiServer.CMS.TinyMce" version="1.0.0" />
          <dependency id="Microsoft.Net.Http" version="2.2.19" />
        </dependencies>
    </metadata>
    <files>
        <file src="bin\$configuration$\Geta.EPi.Imageshop.dll" target="lib\net461\" />
        <file src="bin\$configuration$\Geta-EPi-Imageshop.zip" target="content\modules\_protected\geta-epi-imageshop\" />
        <file src="TinyMce\ClientResources\editor_plugin.js" target="content\Util\Editor\tinymce\plugins\getaepiimageshop\" />
        <file src="TinyMce\ClientResources\InsertImage.aspx" target="content\Util\Editor\tinymce\plugins\getaepiimageshop\" />
        <file src="TinyMce\ClientResources\images\icon.gif" target="content\Util\Editor\tinymce\plugins\getaepiimageshop\images\" />
        <file src="ImageshopImage.cshtml" target="content\Views\Shared\DisplayTemplates\" />
        <file src="ImageshopVideo.cshtml" target="content\Views\Shared\DisplayTemplates\" />
        <file src="scripts\jwplayer.flash.swf" target="content\scripts\" />
        <file src="scripts\jwplayer.html5.js" target="content\scripts\" />
        <file src="scripts\jwplayer.js" target="content\scripts\" />
        <file src="scripts\jwplayer.imageshop.js" target="content\scripts\" />
        <file src="web.config.install.xdt" target="content\" />
        <file src="web.config.uninstall.xdt" target="content\" />
    </files>
</package>
